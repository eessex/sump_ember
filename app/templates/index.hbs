{{ hasUpcoming}}
<div class="upcoming">
    <h4 class="title">Upcoming</h4>
  {{list-events model=upcoming onFinishArtistCleanUp=(action 'transitionToArtist') onFinishPresenterCleanUp=(action 'transitionToPresenter')}}
</div>

<div class="past">
    {{list-events model=past onFinishArtistCleanUp=(action 'transitionToArtist') onFinishPresenterCleanUp=(action 'transitionToPresenter')}}
</div>
